<resources>

    <style name="AppBaseTheme" parent="android:Theme.Holo.Light">
        <item name="windowActionBar">false</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="AppTheme" parent="AppBaseTheme">
        <!--<item name="android:windowBackground">@color/transparent</item>-->
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="line_v">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.1dip</item>
        <item name="android:background">@color/gray</item>
    </style>

    <style name="main_tab_item2">
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">@dimen/textSize5</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/main_menu_text</item>
        <item name="android:background">@null</item>
    </style>

    <style name="rank_tab_item">
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/rank_menu_text</item>
        <item name="android:background">@null</item>
    </style>

    <style name="txt_titleStyle1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/white</item>
    </style>


    <style name="profile_friend_adapter_reason">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">#ff707070</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">false</item>
        <item name="android:lineSpacingMultiplier">1.1</item>
    </style>

    <style name="profile_header_blue_text">
        <item name="android:textSize">13.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/profile_header_text_blue</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="profile_header_black_text">
        <item name="android:textSize">13.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/profile_header_text_black</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="NotificationDate">
        <item name="android:textColor">?android:textColorSecondary</item>
    </style>

    <style name="NotificationTitle">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">?android:textColorPrimary</item>
    </style>

    <style name="NotificationText">
        <item name="android:textColor">?android:textColorPrimary</item>
    </style>


    <style name="my_page_item_root" parent="onCLick">
        <item name="android:paddingLeft">20.0dip</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50.5dip</item>
    </style>

    <style name="onCLick">
        <item name="android:background">@drawable/click_white_gray</item>
    </style>

    <style name="my_page_item_tv">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/ssxinzi2</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="my_page_item_right_arrow">
        <item name="android:paddingRight">17.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/arrow_right_setup</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="my_page_item_divider">
        <item name="android:background">@color/ssxinxian1</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.5dip</item>
    </style>

    <style name="my_page_tagview">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">8.0dip</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="title_bar_style">
        <item name="android:background">@drawable/bg_titlebar</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/title_bar_height</item>
    </style>

    <style name="back_view">
        <item name="android:background">@drawable/transparent</item>
        <item name="android:paddingLeft">10.0dip</item>
        <item name="android:paddingRight">0.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">0.0dip</item>
        <item name="android:layout_marginRight">0.0dip</item>
        <item name="android:minHeight">44.0dip</item>
        <item name="android:text">" "</item>
        <item name="android:drawableLeft">@drawable/btn_back</item>
        <item name="android:drawablePadding">0.0dip</item>
    </style>

    <style name="title_text_style">
        <item name="android:textSize">17.0sp</item>
        <item name="android:textColor">#ff222222</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">2.0dip</item>
    </style>

    <style name="title_bar_button" parent="@style/common_button">
        <item name="android:textSize">17.0sp</item>
        <item name="android:textColor">@color/btn_common_text</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@null</item>
        <item name="android:paddingLeft">15.0dip</item>
        <item name="android:paddingRight">15.0dip</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="common_button" parent="@style/text_on_button">
        <item name="android:background">@drawable/btn_common</item>
        <item name="android:paddingLeft">6.0dip</item>
        <item name="android:paddingRight">6.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minWidth">50.0dip</item>
        <item name="android:minHeight">30.0dip</item>
    </style>

    <style name="text_on_button">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:shadowRadius">0.0</item>
    </style>

    <style name="detail_more_title_view">
        <item name="android:background">@drawable/transparent</item>
        <item name="android:paddingLeft">5.0dip</item>
        <item name="android:paddingRight">15.0dip</item>
        <item name="android:layout_marginLeft">0.0dip</item>
        <item name="android:minHeight">@dimen/title_bar_height</item>
        <item name="android:text"></item>
        <item name="android:drawableRight">@drawable/btn_more_title_detail</item>
        <item name="android:drawablePadding">0.0dip</item>
    </style>

    <style name="item_setting_middle" parent="@style/item_setting">
        <item name="android:background">@drawable/item_setting_middle</item>
    </style>

    <style name="item_setting">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">50.0dip</item>
    </style>

    <style name="setting_item_text">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ff222222</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">15.0dip</item>
        <item name="android:layout_marginRight">10.0dip</item>
    </style>

    <style name="setting_alt_text">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@color/ssxinzi3</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">5.0dip</item>
        <item name="android:layout_marginRight">15.0dip</item>
        <item name="android:layout_weight">0.0</item>
    </style>

    <style name="item_setting_bottom" parent="@style/item_setting">
        <item name="android:background">@drawable/item_setting_bottom</item>
    </style>

    <style name="setting_hint_new">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">5.0dip</item>
        <item name="android:src">@drawable/icon_new</item>
        <item name="android:layout_weight">0.0</item>
    </style>

    <style name="setting_item_head">
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/bg_row_head</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">42.0dip</item>
    </style>

    <style name="setting_release_info">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">@color/setting_copyright</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">6.0dip</item>
        <item name="android:layout_marginBottom">8.0dip</item>
    </style>

    <style name="setting_copyright">
        <item name="android:textSize">13.0sp</item>
        <item name="android:textColor">@color/setting_copyright</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10.0dip</item>
        <item name="android:text">@string/app_copyright</item>
    </style>

    <style name="SwitchButtonStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:layout_marginRight">15.0dip</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <style name="MaterialDesignStyle" parent="SwitchButtonStyle">
        <item name="animationVelocity">0</item>
        <item name="insetBottom">0dp</item>
        <!--设置上下背景-->
        <item name="insetTop">0dp</item>
        <item name="insetLeft">10dp</item>
        <item name="insetRight">10dp</item>
        <item name="measureFactor">1.8</item>
        <item name="offDrawable">@drawable/md_back_off</item>
        <item name="onDrawable">@drawable/md_back_on</item>
        <item name="thumbDrawable">@drawable/md_thumb</item>
        <item name="thumb_margin">-6dp</item>
    </style>

    <style name="main_title_bar_style">
        <item name="android:background">@drawable/bg_titlebar_main</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/title_bar_height</item>
    </style>

    <style name="top_update_count">
        <item name="android:textSize">10.0sp</item>
        <item name="android:textColor">#ffcc3131</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">3.0dip</item>
        <item name="android:paddingRight">3.0dip</item>
        <item name="android:minWidth">15.0dip</item>
        <item name="android:minHeight">15.0dip</item>
    </style>

    <style name="loadingProgressBar" parent="android:Widget.Holo.ProgressBar.Large" />
<style name="title_right_button">
<item name="android:paddingLeft">8dp</item>
<item name="android:paddingRight">8dp</item>
<item name="android:clickable">true</item>
<item name="android:scaleType">fitCenter</item>
<item name="android:background">@drawable/back_button_default_style</item>
</style>
</resources>
